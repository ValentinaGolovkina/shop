<% layout('./layout/page') -%>
<% block('title', 'Valensiya - Женская одежда'); %>
<div class="product">
    <div class="container-fluid text-center">
        <div class="row ">
            <div class="col2 col-xs-4 col-sm-3 col-md-2">
                <% if (section=="Ж") {%>
                <%-partial('./partials/women')%>
                <% } else if (section=="М") {%>
                <%-partial('./partials/men')%>
                <% } else if (section=="Д") {%>
                <%-partial('./partials/children')%>
                <% } %>
            </div>
            <div class="col-xs-8 col-sm-9 col-md-10 panel-default">
                <ul class="thumbnails list-unstyled">
                    <% var count=goods.length;%>
                    <% if (count==0) {%>
                    <h4>Товаров не найдено</h4>
                    <%} else while (count>0) {%>
                    <%-partial('./partials/rowMiniature',{goods:goods, count:count})%>
                    <% count = count-4; }%>
                </ul>
            </div>
        </div>
    </div>
</div>